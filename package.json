    "if-env": "^1.0.4",
    "joi": "^17.3.0",
    "joi-objectid": "^3.0.1",
    "joi-password-complexity": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.5"
  },
